rbox:ratatouille a rbox:MainDish, rbox:VeganRecipe ;
    # general attributes of the recipe
        rbox:hasID "R006" ;
        rbox:hasTitle "Ratatouille" ;
        rbox:hasDescription "A traditional French Provençal stewed vegetable dish, originating in Nice." ;
        schema:recipeYield "4 servings" ;
        rbox:hasCuisine rbox:FrenchCuisine ;

    #properties about the ingredients
        # no allergies
        rbox:hasIngredientList (
            [ rbox:hasIngredient rbox:Eggplant ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Zucchini ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:BellPeppers ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Tomatoes ; rbox:hasQuantity 4 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Onions ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Garlic ; rbox:hasQuantity 3 ; rbox:hasUnit rbox:cloves ]
            [ rbox:hasIngredient rbox:OliveOil ; rbox:hasQuantity 3 ; rbox:hasUnit rbox:tablespoon ]
            [ rbox:hasIngredient rbox:HerbsDeProvence ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:teaspoon ]);  

    #properties of the preparation
        schema:prepTime "PT1H" ;
        rbox:hasInstructions """1. Slice eggplant, zucchini, bell peppers, and tomatoes.
        2. Sauté onions and garlic in olive oil until translucent.
        3. Layer the sliced vegetables in a baking dish.
        4. Drizzle with olive oil and sprinkle with Herbs de Provence.
        5. Bake at 190°C for about 45 minutes until vegetables are tender.""" .

rbox:ratatouille_tart a rbox:Appetizer ;
    # general attributes of the recipe
        rbox:hasID "R013" ;
        rbox:hasTitle "Ratatouille Tart" ;
        rbox:hasDescription "A French appetizer tart with layers of colorful vegetables, inspired by classic ratatouille." ;
        schema:recipeYield "4 servings" ;
        rbox:hasCuisine rbox:FrenchCuisine ;

    # properties about the ingredients
        rbox:hasAllergenInfo rbox:Gluten, rbox:Milk ;
        rbox:hasIngredientList (
            [ rbox:hasIngredient rbox:PastryDough ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Zucchini ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Eggplant ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:BellPeppers ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Tomatoes ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
            [ rbox:hasIngredient rbox:Cheese ; rbox:hasQuantity 50 ; rbox:hasUnit rbox:grams ]) ;

    # properties of the preparation
        schema:prepTime "PT50M" ;
        rbox:hasInstructions """1. Preheat oven to 180°C.
        2. Roll out pastry dough and place in tart pan.
        3. Slice vegetables thinly and layer them decoratively on the dough.
        4. Sprinkle with cheese and bake for 35-40 minutes until vegetables are tender and pastry is golden.
        5. Serve warm.""" .

rbox:creme_brulee a rbox:Dessert, rbox:VegetarianRecipe ;
    rbox:hasID "R010" ;
    rbox:hasTitle "Crème Brûlée" ;
    rbox:hasDescription "A classic French dessert consisting of rich custard topped with a layer of hard caramel." ;
    schema:recipeYield "4 servings" ;
    rbox:hasCuisine rbox:FrenchCuisine ;
    rbox:hasAllergenInfo rbox:Milk, rbox:EggAllergen ;
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:Eggs ; rbox:hasQuantity 4 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Sugar ; rbox:hasQuantity 100 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Milk ; rbox:hasQuantity 500 ; rbox:hasUnit rbox:milliliter ]
        [ rbox:hasIngredient rbox:VanillaExtract ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:teaspoon ]
        [ rbox:hasIngredient rbox:PowderedSugar ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:tablespoon ]
    ) ;
    schema:prepTime "PT50M" ;
    rbox:hasInstructions """1. Preheat oven to 150°C.
    2. Beat egg yolks with sugar until pale.
    3. Heat milk with vanilla and pour slowly into egg mixture.
    4. Pour custard into ramekins and bake in a water bath for 40 minutes.
    5. Cool, sprinkle sugar on top, and caramelize with a torch before serving.""" .

