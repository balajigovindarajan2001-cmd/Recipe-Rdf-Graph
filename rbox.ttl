    #properties about the ingredients
        rbox:hasAllergenInfo rbox:Gluten, rbox:EggAllergen ;
        rbox:hasIngredientList (
            [ rbox:hasIngredient rbox:Rice ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:cup ]
