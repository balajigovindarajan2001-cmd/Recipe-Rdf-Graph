# this is the schema of the projects what are the relationships between them
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rbox: <http://example.org/rbox#> .
# @prefix owl:  <http://www.w3.org/2002/07/owl#> .

# classes
rbox:Recipe a rdfs:Class ;
    rdfs:label "Recipe"@en, "Rezept"@de ;
    rdfs:comment "A culinary recipe with ingredients, instructions."@en,
                 "Ein kulinarisches Rezept mit Zutaten und Anweisungen."@de ;
    rdfs:subClassOf schema:Recipe .

rbox:IngredientBundle a rdfs:Class ;
    rdfs:label "Ingredient bundle"@en, "Sammlung an Zutaten"@de;
    rdfs:comment "A combination of ingredient, quantity, and unit."@en, "Kombination von Zutaten deren Quantität und dessen Einheit"@de.

rbox:Ingredient a rdfs:Class ;
    rdfs:label "Ingredient"@en, "Zutat"@de ;
    rdfs:comment "An ingredient used in a recipe."@en,
                 "Eine Zutat, die in einem Rezept verwendet wird."@de .
    
#subclasses of Ingredient
    rbox:Vegetable a rdfs:Class ;
        rdfs:label "Vegetable"@en, "Gemüse"@de ;
        rdfs:comment "A plant or part of a plant used as food."@en,
                     "Eine Pflanze oder ein Teil einer Pflanze, die als Nahrungsmittel verwendet wird."@de ;
        rdfs:subClassOf rbox:Ingredient .

    rbox:Fruit a rdfs:Class ;
        rdfs:label "Fruit"@en, "Obst"@de ;
        rdfs:comment "The sweet and fleshy product of a tree or other plant that contains seed and can be eaten as food."@en,
                     "Das süße und fleischige Produkt eines Baumes oder einer anderen Pflanze, das Samen enthält und als Nahrungsmittel gegessen werden kann."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Spice a rdfs:Class ;
        rdfs:label "Spice"@en, "Gewürz"@de ;
        rdfs:comment "A dried seed, fruit, root, bark, or vegetable substance used for flavoring, coloring or preserving food."@en,
                     "Ein getrockneter Samen, eine Frucht, eine Wurzel, Rinde oder eine pflanzliche Substanz, die zum Würzen, Färben oder Konservieren von Lebensmitteln verwendet wird."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:DairyProduct a rdfs:Class ;
        rdfs:label "Dairy Product"@en, "Milchprodukt"@de ;
        rdfs:comment "A food product made from or containing milk."@en,
                     "Ein Lebensmittelprodukt, das aus Milch hergestellt wird oder Milch enthält."@de ;
        rdfs:subClassOf rbox:Ingredient .

    rbox:Meat a rdfs:Class ;
        rdfs:label "Meat"@en, "Fleisch"@de ;
        rdfs:comment "The flesh of animals used as food."@en,
                     "Das Fleisch von Tieren, das als Nahrungsmittel verwendet wird."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Grain a rdfs:Class ;
        rdfs:label "Grain"@en, "Getreide"@de ;
        rdfs:comment "Seeds of cereal plants used as food."@en,
                     "Samen von Getreidepflanzen, die als Nahrungsmittel verwendet werden."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Herb a rdfs:Class ;
        rdfs:label "Herb"@en, "Kräuter"@de ;
        rdfs:comment "A plant or plant part used for flavoring."@en,
                     "Eine Pflanze oder ein Pflanzenteil, der zum Würzen oder als Medizin verwendet wird."@de ;
        rdfs:subClassOf rbox:Ingredient .

    rbox:Sauce a rdfs:Class ;
        rdfs:label "Sauce"@en, "Soße"@de ;
        rdfs:comment "A liquid or semi-liquid substance served with food to add flavor."@en,
                     "Eine flüssige oder halbfeste Substanz, die mit Lebensmitteln serviert wird, um Geschmack hinzuzufügen."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Oil a rdfs:Class ;
        rdfs:label "Oil"@en, "Öl"@de ;
        rdfs:comment "A viscous liquid derived from plants or animals, used in cooking."@en,
                     "Eine viskose Flüssigkeit, die aus Pflanzen oder Tieren gewonnen wird und zum Kochen verwendet wird."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Legumes a rdfs:Class ;
        rdfs:label "Legumes"@en, "Hülsenfrüchte"@de ;
        rdfs:comment "A class of vegetables that includes beans, lentils, and peas."@en,
                     "Eine Klasse von Gemüsesorten, zu der Bohnen, Linsen und Erbsen gehören."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Nut a rdfs:Class ;
        rdfs:label "Nut"@en, "Nuss"@de ;
        rdfs:comment "A hard-shelled fruit or seed that is edible."@en,
                     "Eine hartschalige Frucht oder Samen, die essbar ist."@de ;
        rdfs:subClassOf rbox:Ingredient.
    
    rbox:Sefood a rdfs:Class ;
        rdfs:label "Seafood"@en, "Meeresfrüchte"@de ;
        rdfs:comment "Edible marine animals and plants."@en,
                     "Essbare Meerestiere und Pflanzen."@de ;
        rdfs:subClassOf rbox:Ingredient .

    rbox:EggProduct a rdfs:Class ;
        rdfs:label "Egg Product"@en, "Eierprodukt"@de ;
        rdfs:comment "A food product made from or containing eggs."@en,
                     "Ein Lebensmittelprodukt, das aus Eiern hergestellt wird oder Eier enthält."@de ;
        rdfs:subClassOf rbox:Ingredient .
    
    rbox:Drink a rdfs:Class ;
        rdfs:label "Drink"@en, "Getränk"@de ;
        rdfs:comment "A liquid intended for consumption, such as water, coffee, tea, or juice."@en,
                     "Eine Flüssigkeit, die zum Verzehr gedacht ist, wie Wasser, Kaffee, Tee oder Saft."@de ;
        rdfs:subClassOf rbox:Ingredient .


rbox:Menu a rdfs:Class ; # still link it
    rdfs:label "Menu"@en, "Menü"@de ;
    rdfs:comment "A collection of recipes that go well as a menu together. A buyable product."@en,
                 "Eine Sammlung von Rezepten, die zusammen serviert werden können und als ein Produkt verkauft werden können."@de .
    
    rbox:hasRecipe a rdf:Property ;
        rdfs:label "has recipe"@en, "hat Rezept"@de ;
        rdfs:domain rbox:Menu ;
        rdfs:range rbox:Recipe .


rbox:Unit a rdfs:Class ;
    rdfs:label "Unit"@en, "Einheit"@de ;
    rdfs:comment "A measurement unit used to express the amount of an ingredient."@en ,
                 "Eine Maßeinheit, um die Menge einer Zutat auszudrücken."@de .

    # subclasses of unit 
    rbox:MassUnit a rdfs:Class ;
        rdfs:label "Mass Unit"@en ;
        rdfs:comment "A unit used to measure mass, e.g., grams or kilograms."@en; 
        rdfs:subClassOf rbox:Unit .

    rbox:VolumeUnit a rdfs:Class ;
        rdfs:label "Volume Unit"@en ;
        rdfs:comment "A unit used to mfeasure volume, e.g., liters or milliliters."@en;
        rdfs:subClassOf rbox:Unit .

    rbox:CountUnit a rdfs:Class ;
        rdfs:label "Count Unit"@en ;
        rdfs:comment "A unit representing countable items, e.g., pieces or items."@en; 
        rdfs:subClassOf rbox:Unit .

    rbox:LengthUnit a rdfs:Class ;
        rdfs:label "Length Unit"@en ;
        rdfs:comment "Units that measure length (e.g., inch) when relevant in recipes."@en ; 
        rdfs:subClassOf rbox:Unit .

    rbox:MiscUnit a rdfs:Class ;
        rdfs:label "Miscellaneous Unit"@en ;
        rdfs:comment "Non-standard/casual measures (e.g., pinch, handful, dash)."@en ; 
        rdfs:subClassOf rbox:Unit .

# properties of Unit
    rbox:amount a rdf:Property ;
        rdfs:domain rbox:Unit ;
        rdfs:range xsd:decimal ;
        rdfs:label "amount"@en ;
        rdfs:comment "Numeric amount associated with this unit instance."@en .

    rbox:unitName a rdf:Property ;
        rdfs:domain rbox:Unit ;
        rdfs:range xsd:string ;
        rdfs:label "unit name"@en ;
        rdfs:comment "Human-readable name of the unit (e.g., 'gram', 'cup', 'inch'). Useful for display."@en .

    rbox:unitType a rdf:Property ;
        rdfs:domain rbox:Unit ;
        rdfs:range rbox:Unit ;
        rdfs:label "unit type"@en ;
        rdfs:comment "Optional: points to a canonical unit individual/type if you want reuse."@en .

# links from recipe via hasIngredientList and IngredientBundle to Ingredients 
    rbox:hasIngredientList a rdf:Property ;
        rdfs:domain rbox:Recipe ;
        rdfs:range rdf:List;
        rdfs:label "has ingredient list"@en, "Zutatenliste"@de ;
        rdfs:comment "Links a recipe to its ingredient+amount+unit tuples."@en, "Verbindet Zutaten mit Quantität und Einheit".

        rbox:hasIngredient a rdf:Property ;
            rdfs:domain rbox:IngredientBundle ;
            rdfs:range rbox:Ingredient ;
            rdfs:label "has ingredient"@en, "hat Zutat"@de ;
            rdfs:comment "The ingredient in the recipe ingredient tuple."@en, "Die Zutat als triple in der Ingredientliste" .

        rbox:hasQuantity a rdf:Property ;
            rdfs:domain rbox:IngredientBundle;
            rdfs:range xsd:decimal ;
            rdfs:label "has quantity"@en, "hat Quantität"@de ;
            rdfs:comment "The numeric amount of the ingredient in the recipe."@en, "der Zahlenwert zugehörig zu einer Einheit"@de .

        rbox:hasUnit a rdf:Property ;
            rdfs:domain rbox:IngredientBundle ;
            rdfs:range rbox:Unit ;
            rdfs:label "has unit"@en, "hat Einheit"@de ;
            rdfs:comment "The unit of measurement for this ingredient amount."@en, "D" .

# Subclases of Recipe: 
# type of recipes
    rbox:VegetarianRecipe a rdfs:Class ;
        rdfs:label "Vegetarian Recipe"@en, "Vegetarisches Rezept"@de ;
        rdfs:comment "A recipe that does not include meat or fish."@en,
                     "Ein Rezept, das kein Fleisch oder Fisch enthält."@de ;
        rdfs:subClassOf rbox:Recipe .

    rbox:VeganRecipe a rdfs:Class ;
        rdfs:label "Vegan Recipe"@en, "Veganes Rezept"@de ;
        rdfs:comment "A recipe that does not include any animal products."@en,
                     "Ein Rezept, das keine tierischen Produkte enthält."@de ;
        rdfs:subClassOf rbox:Recipe .   
    

# course type 
    rbox:Course a rdfs:class ; 
        rdfs:label "Course"@en, "Kurs"@de ;
        rdfs:comment "A type of course for a recipe."@en,
                     "Eine Art von Gericht für ein Rezept."@de ;
        rdfs:subClassOf rbox:Recipe .

    rbox:Appetizer a rdfs:Class ;
        rdfs:label "Appetizer"@en, "Vorspeise"@de ;
        rdfs:comment "A recipe for a starter dish."@en,
                     "Ein Rezept für eine Vorspeise."@de ;
        rdfs:subClassOf rbox:course .
    
    rbox:MainDish a rdfs:Class ;
        rdfs:label "Main Dish"@en, "Hauptgericht"@de ;
        rdfs:comment "A recipe for a main course."@en,
                     "Ein Rezept für ein Hauptgericht."@de ;
        rdfs:subClassOf rbox:course .
    
    rbox:Dessert a rdfs:Class ;
        rdfs:label "Dessert"@en, "Nachtisch"@de ;
        rdfs:comment "A recipe for a dessert."@en,
                     "Ein Rezept für einen Nachtisch."@de ;
        rdfs:subClassOf rbox:course .

# cuisine type
    rbox:Cuisine a rdfs:Class ;
        rdfs:label "Cuisine"@en, "Küche"@de ;
        rdfs:comment "A type of cuisine for a recipe."@en,
                     "Eine Art von Küche für ein Rezept."@de .

# recipe properties
    rbox:hasID a rdfs:DatatypeProperty ;
        rdfs:domain rbox:Recipe, rbox:Menu;
        rdfs:range xsd:string ; 
        rdfs:label "has ID"@en, "hat ID"@de ;
        rdfs:comment "A unique identifier for the recipe."@en,
                 "Eine eindeutige Kennung für das Rezept."@de .

    rbox:hasTitle a rdfs:DatatypeProperty ;
        rdfs:domain rbox:Recipe, rbox:Menu;
        rdfs:range xsd:string ;
        rdfs:label "has title"@en, "hat Titel"@de ;
        rdfs:comment "The title of the recipe."@en,
                 "Der Titel des Rezepts."@de .

    rbox:hasDescription a rdfs:DatatypeProperty ;
        rdfs:domain rbox:Recipe, rbox:Menu ;
        rdfs:range xsd:string ;
        rdfs:label "has description"@en, "hat Beschreibung"@de ;
        rdfs:comment "A brief description of the recipe."@en,
                 "Eine kurze Beschreibung des Rezepts."@de .
    
    rbox:hasInstructions a rdfs:DatatypeProperty ;
        rdfs:domain rbox:Recipe ;
        rdfs:range xsd:string ;
        rdfs:label "has instructions"@en, "hat Anweisungen"@de ;
        rdfs:comment "The preparation instructions for the recipe."@en,
                     "Die Zubereitungsanweisungen für das Rezept."@de .

    rbox:hasCuisine a rdfs:ObjectProperty ;
        rdfs:domain rbox:Recipe, rbox:Menu ;
        rdfs:range rbox:Cuisine ;
        rdfs:label "has cuisine"@en, "gehört zur Küche"@de ;
        rdfs:comment "Specifies the cuisine type of the recipe."@en,
                     "Gibt die Küchenart des Rezepts an."@de .
        
    #ingredient properties
    rbox:hasName a rdfs:DatatypeProperty ;
        rdfs:domain rbox:Ingredient ;
        rdfs:range xsd:string ;
        rdfs:label "has name"@en, "hat Name"@de ;
        rdfs:comment "The name of the ingredient."@en,
                     "Der Name der Zutat."@de .
                     
rbox:hasAllergenInfo a rdfs:ObjectProperty ;
    rdfs:domain rbox:Recipe , rbox:Ingredient ;
    rdfs:range rbox:Allergen ; 
    rdfs:label "has allergen info"@en, "hat Allergeninformation"@de ;
    rdfs:comment "Provides allergen information for the recipe."@en,
                 "Bietet Allergeninformationen für das Rezept."@de .
    #owl:propertyChainAxiom ( rbox:hasIngredient rbox:hasAllergenInfo ) .

# Balajis Ontology part
# Classes
# Customer
rbox:Customer a rdfs:Class ;
    rdfs:subClassOf schema:Person ;
    rdfs:label "Customer"@en ;
    rdfs:comment "A customer subscribed to the recipe box service."@en .

# Courier
rbox:Courier a rdfs:Class ;
    rdfs:subClassOf schema:Person ;
    rdfs:label "Courier"@en ;
    rdfs:comment "A delivery person responsible for delivering recipe boxes."@en .

# Subscription Plan
rbox:SubscriptionPlan a rdfs:Class ;
    rdfs:label "Subscription Plan"@en ;
    rdfs:comment "A pricing plan selected by a customer, e.g., 3 recipes x 2 people."@en .

# Order
rbox:Order a rdfs:Class ;
    rdfs:subClassOf schema:Order ;
    rdfs:label "Order"@en ;
    rdfs:comment "An order placed by a customer for one or more recipes."@en .

# Order Line
rbox:OrderLine a rdfs:Class ;
    rdfs:subClassOf schema:OrderItem ;
    rdfs:label "Order Line"@en ;
    rdfs:comment "A line item in an order linking a selected recipe and quantity."@en .

# Delivery
rbox:Delivery a rdfs:Class ;
    rdfs:subClassOf schema:DeliveryEvent ;
    rdfs:label "Delivery"@en ;
    rdfs:comment "A delivery event for a customer order, executed by a courier."@en .

# Review
rbox:Review a rdfs:Class ;
    rdfs:subClassOf schema:Review ;
    rdfs:label "Review"@en ;
    rdfs:comment "A customer review of a recipe, menu or delivery."@en .

# Object Properties

# Customer ↔ SubscriptionPlan
rbox:hasSubscriptionPlan a rdf:Property ;
    rdfs:label "has subscription plan"@en ;
    rdfs:domain rbox:Customer ;
    rdfs:range rbox:SubscriptionPlan ;
    rdfs:comment "Links a customer to their selected subscription plan."@en .

# Order ↔ Customer
rbox:orderedBy a rdf:Property ;
    rdfs:label "ordered by"@en ;
    rdfs:subPropertyOf schema:customer ;
    rdfs:domain rbox:Order ;
    rdfs:range rbox:Customer ;
    rdfs:comment "Associates an order with the customer who placed it."@en .

# Order ↔ OrderLine
rbox:hasOrderLine a rdf:Property ;
    rdfs:label "has order line"@en ;
    rdfs:domain rbox:Order ;
    rdfs:range rbox:OrderLine ;
    rdfs:comment "Links an order to each of its line items."@en .

# Delivery ↔ Order
rbox:deliveryForOrder a rdf:Property ;
    rdfs:label "delivery for order"@en ;
    rdfs:domain rbox:Delivery ;
    rdfs:range rbox:Order ;
    rdfs:comment "Links a delivery event to the order being delivered."@en .

# Delivery ↔ Courier
rbox:deliveredBy a rdf:Property ;
    rdfs:label "delivered by"@en ;
    rdfs:domain rbox:Delivery ;
    rdfs:range rbox:Courier ;
    rdfs:comment "Indicates which courier delivered the order."@en .

# Delivery ↔ Customer
rbox:deliveredTo a rdf:Property ;
    rdfs:label "delivered to"@en ;
    rdfs:domain rbox:Delivery ;
    rdfs:range rbox:Customer ;
    rdfs:comment "Indicates which customer received the delivery."@en .

# Order ↔ Review
rbox:hasReview a rdf:Property ;
    rdfs:label "has review"@en ;
    rdfs:domain rbox:Order ;
    rdfs:range rbox:Review ;
    rdfs:comment "Links an order to its customer-written review."@en .

# Review ↔ Customer
rbox:reviewAuthor a rdf:Property ;
    rdfs:label "review author"@en ;
    rdfs:subPropertyOf schema:author ;
    rdfs:domain rbox:Review ;
    rdfs:range rbox:Customer ;
    rdfs:comment "Indicates the customer who authored the review."@en .

# Properties

# OrderLine quantity
rbox:portionCount a rdf:Property ;
    rdfs:label "portion count"@en ;
    rdfs:domain rbox:OrderLine ;
    rdfs:range xsd:integer ;
    rdfs:comment "The number of portions ordered for this recipe."@en .

# Delivery start date
rbox:deliveryStart a rdf:Property ;
    rdfs:label "delivery start date/time"@en ;
    rdfs:domain rbox:Delivery ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "Start time of the delivery window."@en .

# Delivery end date
rbox:deliveryEnd a rdf:Property ;
    rdfs:label "delivery end date/time"@en ;
    rdfs:domain rbox:Delivery ;
    rdfs:range xsd:dateTime ;
    rdfs:comment "End time of the delivery window."@en .

# Subscription plan description
rbox:planLabel a rdf:Property ;
    rdfs:label "subscription plan label"@en ;
    rdfs:domain rbox:SubscriptionPlan ;
    rdfs:range xsd:string ;
    rdfs:comment "Human-readable label of the subscription plan."@en .

# Subscription plan price
rbox:planPrice a rdf:Property ;
    rdfs:label "subscription plan price"@en ;
    rdfs:domain rbox:SubscriptionPlan ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Price of the subscription plan per week."@en .

# Popularity

# How many orders are associated with this subscription plan
rbox:planOrderCount a rdf:Property ;
    rdfs:label "plan order count"@en ;
    rdfs:comment "Total number of orders placed by customers using this subscription plan."@en ;
    rdfs:domain rbox:SubscriptionPlan ;
    rdfs:range xsd:integer .

# How many reviews given by customers on this plan's orders
rbox:planReviewCount a rdf:Property ;
    rdfs:label "plan review count"@en ;
    rdfs:comment "Total number of reviews written for orders of this subscription plan."@en ;
    rdfs:domain rbox:SubscriptionPlan ;
    rdfs:range xsd:integer .

# Average rating of those reviews (0–5 for example)
rbox:planAverageRating a rdf:Property ;
    rdfs:label "plan average rating"@en ;
    rdfs:comment "Average rating for orders associated with this subscription plan."@en ;
    rdfs:domain rbox:SubscriptionPlan ;
    rdfs:range xsd:decimal .

# Combined popularity score (your own formula)
rbox:planPopularityScore a rdf:Property ;
    rdfs:label "plan popularity score"@en ;
    rdfs:comment "Combined score based on order frequency and reviews for this subscription plan."@en ;
    rdfs:domain rbox:SubscriptionPlan ;
    rdfs:range xsd:decimal .


