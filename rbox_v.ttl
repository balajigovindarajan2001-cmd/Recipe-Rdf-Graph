rbox:spaghetti_carbonara a rbox:MainDish;
    # properties of the recipe
    rbox:hasID "recipe001" ;
    rbox:hasTitle "Spaghetti Carbonara" ;
    rbox:hasDescription "A classic Italian pasta dish made with eggs, cheese, pancetta, and pepper." ;
    
    schema:recipeYield "4 servings" ;
    rbox:hasCuisine rbox:ItalianCuisine ;

    #properties of the ingredients
    rbox:hasAllergenInfo rbox:Gluten, rbox:EggAllergen, rbox:DairyProduct, rbox:Pork ;
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:Spaghetti ; rbox:hasQuantity 400 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Pancetta ; rbox:hasQuantity 150 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Eggs ; rbox:hasQuantity 4 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:ParmesanCheese ; rbox:hasQuantity 100 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:BlackPepper ]
    ) ;

    #properties of the preparation
    schema:prepTime "PT20M" ;
    rbox:hasInstructions """1. Cook spaghetti according to package instructions. 
    2. In a pan, cook pancetta until crispy.
    3. Beat eggs and mix with grated Parmesan cheese.
    4. Combine hot spaghetti with pancetta and egg mixture, stirring quickly to create a creamy sauce. 
    5. Season with black pepper and serve immediately.""" .

rbox:briyani a rbox:MainDish ;
    # properties of the recipe
    rbox:hasID "recipe002" ;
    rbox:hasTitle "Chicken Biryani" ;
    rbox:hasDescription "A flavorful Indian rice dish made with marinated chicken, spices, and basmati rice." ;
    
    schema:recipeYield "6 servings" ;
    rbox:hasCuisine rbox:IndianCuisine ;

    #properties of the ingredients
    rbox:hasAllergenInfo rbox:Gluten, rbox:DairyProduct ;
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:BasmatiRice ; rbox:hasQuantity 500 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Chicken ; rbox:hasQuantity 1000 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Yogurt ; rbox:hasQuantity 200 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Onions ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Garlic ; rbox:hasQuantity 4 ; rbox:hasUnit rbox:cloves ]
        [ rbox:hasIngredient rbox:Ginger ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:inch ] 
        [ rbox:hasIngredient rbox:Spices ]
    ); 

    #properties of the preparation
    schema:prepTime "PT1H30M" ;
    rbox:hasInstructions """1. Marinate chicken with yogurt and spices.
    2. Fry onions until golden brown.
    3. Layer marinated chicken and partially cooked basmati rice in a pot.
    4. Cook on low heat until chicken is tender and rice is fully cooked.
    5. Garnish with fried onions and serve hot.""" .

rbox:spaghetti_bolognese a rbox:MainDish;
    # properties of the recipe
    rbox:hasID "recipe003" ;
    rbox:hasTitle "Spaghetti Bolognese" ;
    rbox:hasDescription "A traditional Italian pasta dish with a rich meat sauce." ;
    
    schema:recipeYield "4 servings" ;
    rbox:hasCuisine rbox:ItalianCuisine ;

    #properties of the ingredients
    rbox:hasAllergenInfo rbox:Gluten, rbox:DairyProduct ;
   
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:Spaghetti ; rbox:hasQuantity 500 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:GroundBeef ; rbox:hasQuantity 500 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:TomatoSauce ; rbox:hasQuantity 400 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Onions ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Garlic ; rbox:hasQuantity 3 ; rbox:hasUnit rbox:cloves ]
        [ rbox:hasIngredient rbox:OliveOil ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:tablespoon ]
        [ rbox:hasIngredient rbox:ParmesanCheese ; rbox:hasQuantity 100 ; rbox:hasUnit rbox:grams ]
    ) ;  
    
    #properties of the preparation
    schema:prepTime "PT45M" ;
    rbox:hasInstructions """1. Cook spaghetti according to package instructions. 
    2. In a pan, sauté onions and garlic in olive oil until translucent.
    3. Add ground beef and cook until browned.
    4. Pour in tomato sauce and simmer for 20 minutes.
    5. Serve sauce over spaghetti and top with grated Parmesan cheese.""" .

rbox:Kairserschmarn a rbox:Dessert, rbox:VegetarianRecipe;
    # properties of the recipe
    rbox:hasID "recipe004" ;
    rbox:hasTitle "Kaiserschmarrn" ;
    rbox:hasDescription "A traditional Austrian shredded pancake dessert, often served with fruit compote." ;
    
    schema:recipeYield "2 servings" ;
    rbox:hasCuisine rbox:GermanCuisine ;

    #properties of the ingredients
    rbox:hasAllergenInfo rbox:Gluten, rbox:DairyProduct, rbox:EggAllergen ;
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:Flour ; rbox:hasQuantity 150 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Eggs ; rbox:hasQuantity 4 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Milk ; rbox:hasQuantity 200 ; rbox:hasUnit rbox:milliliter ]
        [ rbox:hasIngredient rbox:Butter ; rbox:hasQuantity 50 ; rbox:hasUnit rbox:grams ]
        [ rbox:hasIngredient rbox:Sugar ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:tablespoon ]
        [ rbox:hasIngredient rbox:Salt ; rbox:hasQuantity "a pinch" ; rbox:hasUnit rbox:MiscUnit ]
        [ rbox:hasIngredient rbox:PowderedSugar ; rbox:hasQuantity "for dusting" ; rbox:hasUnit rbox:MiscUnit ]
    );

    #properties of the preparation
    schema:prepTime "PT30M" ;
    rbox:hasInstructions """1. Mix flour, eggs, milk, sugar, and a pinch of salt to form a batter.
    2. Heat butter in a pan and pour in the batter to make a thick pancake.
    3. Cook until golden brown on both sides, then shred into pieces using two forks.
    4. Sprinkle with powdered sugar and serve with fruit compote.""" .

rbox:fried_rice a rbox:MainDish ;
    # properties of the recipe
    rbox:hasID "recipe005" ;
    rbox:hasTitle "Chinese Fried Rice" ;
    rbox:hasDescription "A popular Chinese dish made with stir-fried rice, vegetables, and eggs." ;
    
    schema:recipeYield "4 servings" ;
    rbox:hasCuisine rbox:ChineseCuisine ;

    #properties of the ingredients
    rbox:hasAllergenInfo rbox:Gluten, rbox:EggAllergen ;
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:Rice ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:cup ]
        [ rbox:hasIngredient rbox:Eggs ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:SoySauce ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:tablespoon ]
        [ rbox:hasIngredient rbox:SesameOil ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:tablespoon ]
        [ rbox:hasIngredient rbox:Peas ; rbox:hasQuantity 0.5 ; rbox:hasUnit rbox:cup ]
        [ rbox:hasIngredient rbox:Carrots ; rbox:hasQuantity 0.5 ; rbox:hasUnit rbox:cup ]
        [ rbox:hasIngredient rbox:Corn ; rbox:hasQuantity 0.5 ; rbox:hasUnit rbox:cup ]
    );

    #properties of the preparation
    schema:prepTime "PT25M" ;
    rbox:hasInstructions """1. Cook rice and let it cool.
    2. In a wok, heat sesame oil and sauté mixed vegetables.
    3. Push vegetables to the side and scramble eggs in the wok.
    4. Add cooked rice and soy sauce, stir-frying everything together.
    5. Serve hot.""" .

# vegan french recipe: 
rbox:Ratatouille a rbox:MainDish, rbox:VeganRecipe ;
    # properties of the recipe
    rbox:hasID "recipe006" ;
    rbox:hasTitle "Ratatouille" ;
    rbox:hasDescription "A traditional French Provençal stewed vegetable dish, originating in Nice." ;
    
    schema:recipeYield "4 servings" ;
    rbox:hasCuisine rbox:FrenchCuisine ;

    #properties of the ingredients
    # no allergies
    rbox:hasIngredientList (
        [ rbox:hasIngredient rbox:Eggplant ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Zucchini ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:BellPeppers ; rbox:hasQuantity 2 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Tomatoes ; rbox:hasQuantity 4 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Onions ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:count ]
        [ rbox:hasIngredient rbox:Garlic ; rbox:hasQuantity 3 ; rbox:hasUnit rbox:cloves ]
        [ rbox:hasIngredient rbox:OliveOil ; rbox:hasQuantity 3 ; rbox:hasUnit rbox:tablespoon ]
        [ rbox:hasIngredient rbox:HerbsDeProvence ; rbox:hasQuantity 1 ; rbox:hasUnit rbox:teaspoon ]
    );  

    #properties of the preparation
    schema:prepTime "PT1H" ;
    rbox:hasInstructions """1. Slice eggplant, zucchini, bell peppers, and tomatoes.
    2. Sauté onions and garlic in olive oil until translucent.
    3. Layer the sliced vegetables in a baking dish.
    4. Drizzle with olive oil and sprinkle with Herbs de Provence.
    5. Bake at 375°F (190°C) for about 45 minutes until vegetables are tender.""" .

# Ingredients declarations
    #Ingredients of Ratatouille and its measurments. 
        rbox:Eggplant a rbox:Vegetable ;
