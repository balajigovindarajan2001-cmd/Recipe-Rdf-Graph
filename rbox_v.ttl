    # Ingredients of Spaghetti Carbonara   
        rbox:Spaghetti a rbox:Grain ; 
            rbox:hasName "Spaghetti" ;
            rbox:hasQuantity "400 grams" ;
            rbox:hasAllergenInfo rbox:Gluten .
        rbox:Pancetta a rbox:Meat ;
            rbox:hasName "Pancetta" ;
            rbox:hasQuantity "150 grams" .

        rbox:Eggs a rbox:EggProduct ;
            rbox:hasName "Eggs" ;
            rbox:hasQuantity "4 large eggs" ;
            rbox:hasAllergenInfo rbox:Eggs .
        rbox:ParmesanCheese a rbox:DairyProduct ;
            rbox:hasName "Parmesan Cheese" ;
            rbox:hasQuantity "100 grams" ;
            rbox:hasAllergenInfo rbox:DairyProduct .

        rbox:BlackPepper a rbox:Spice ;
            rbox:hasName "Black Pepper" ;
            rbox:hasQuantity "to taste" .

    # Ingredients of Chicken Biryani
        rbox:BasmatiRice a rbox:Grain ;
            rbox:hasName "Basmati Rice" ;
            rbox:hasQuantity "500 grams" ;  
            rbox:hasAllergenInfo rbox:Gluten .
        rbox:Chicken a rbox:Meat ;
